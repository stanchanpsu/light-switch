{
    "root": true,
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "sourceType": "module"
    },
    "extends": "eslint:recommended",
    "rules": {
        "no-this-before-super": "error",
        "no-undef": "error",
        "no-unreachable": "error",
        "no-unused-vars": "warn",
        "constructor-super": "error",
        "valid-typeof": "error",
        "quotes": [
            "error",
            "single"
        ],
        "no-console": "off",
        "camelcase": "error",
        "semi": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4,
            {
                "SwitchCase": 1
            }
        ]
    }
}